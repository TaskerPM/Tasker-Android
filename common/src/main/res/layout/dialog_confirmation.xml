<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/background_dialog"
        android:minWidth="343dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_confirmation_dialog_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="42dp"
            android:fontFamily="@font/font_pretendard_medium_500"
            android:gravity="center"
            android:lineSpacingExtra="4dp"
            android:textColor="@color/gray_900"
            android:textSize="15sp"
            tools:text="@string/confirmation_dialog_overlap_title" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="8dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_confirmation_dialog_confirm"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/confirmation_dialog_confirm"
                android:textColor="@color/gray_400"
                android:textSize="15sp" />

        </LinearLayout>
    </LinearLayout>
</layout>